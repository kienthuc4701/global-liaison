{
  "extends": "astro/tsconfigs/base",
  "compilerOptions": {
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "importsNotUsedAsValues": "error",
    "plugins": [
      {
        "name": "@astrojs/ts-plugin"
      }
    ]
  }
}
